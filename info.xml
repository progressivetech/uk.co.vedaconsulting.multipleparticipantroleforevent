<?xml version="1.0"?>
<extension key="uk.co.vedaconsulting.multipleparticipantroleforevent" type="module">
  <file>multipleparticipantroleforevent</file>
  <name>Register Multiple Participants for an event</name>
  <description>This extension enables you to register different participant role for an event</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Priyanka Karan (@ Veda NFP Consulting LTD)</author>
    <email>priyanka@vedaconsulting.co.uk</email>
  </maintainer>
  <releaseDate>2015-06-06</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Multipleparticipantroleforevent</namespace>
    <format>23.02.1</format>
  </civix>
  <urls>
    <url desc="Documentation">https://civicrm.org/blogs/priyankakaran/event-registration-based-participant-roles</url>
  </urls>
  <mixins>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
